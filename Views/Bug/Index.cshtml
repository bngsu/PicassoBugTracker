@{
}
@model List<Bug>

<div class="row mt-3"> </div>
    <div class="col-12">
        <h1>Bug</h1>
</div>
<div class="text-end">
    <a href="/Bug/Add" class="btn btn-success">Submit Bug</a>
</div>

<h2>Bug List</h2>

<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Title</th>
            <th>Description</th>
            <th>Priority</th>
            <th>Reporter</th>
            <th>Status</th>
            <th>Detail</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var bug in Model)
        {
                <tr>
                    <td>@bug.Id</td>
                    <td>@bug.Title</td>
                    <td>@bug.Description</td>
                    <td>@bug.Priority</td>
                    <td>@bug.Reporter</td>
                    <td>@bug.Status</td>
                    <td>
                    <a href="/Bug/Detail/@bug.Id">Detail</a>
                   </td>
                </tr>
        }
    </tbody>
</table>